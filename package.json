{
  "name": "nud3",
  "version": "0.0.1-alpha.13",
  "description": "New D3 composable charts for React",
  "repository": "https://github.com/inlineblock/nud3",
  "main": "lib/index",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rm -rf lib",
    "build": "babel src/ -d lib",
    "watch": "babel src/ -d lib -w",
    "start": "node docs/server.js",
    "build-examples": "webpack --config docs/webpack.config.js ",
    "lint": "eslint src/",
    "jest": "jest",
    "jest-watch": "jest --watch",
    "test": "npm run lint && npm run jest"
  },
  "files": [
    "lib/",
    "stylesheets/"
  ],
  "keywords": [
    "NUVI",
    "D3",
    "React"
  ],
  "author": "dennis@inlineblock.com",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-addons-shallow-compare": "^15.4.0"
  },
  "dependencies": {
    "d3": "^4.2.6",
    "lodash": "4.16.3",
    "moment": "^2.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "basicgrid": "^2.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "file-loader": "^0.9.0",
    "jest": "^16.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "numeral": "^1.5.3",
    "prismjs": "^1.5.1",
    "raw-loader": "^0.5.1",
    "react": "^15.4.0",
    "react-addons-shallow-compare": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.4.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "topojson": "^1.6.27",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testPathDirs": [
      "test"
    ],
    "testRegex": "(/__tests__/.*|\\.?(Test|test|spec))\\.jsx?$"
  }
}
